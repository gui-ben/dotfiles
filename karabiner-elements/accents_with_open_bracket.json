{
    "title": "open bracket + vocal = acute accented vocal (Spanish accent) (analogous for ñ)",
    "rules": [
        {
            "description": "open bracket + a = á",
            "manipulators": [
                {
                    "from": {
                        "key_code": "open_bracket"
                    },
                    "to": [
                        {
                            "set_variable": {
                                "name": "open_bracket_modifier",
                                "value": 1
                            }
                        }
                    ],
                    "to_after_key_up": [
                        {
                            "set_variable": {
                                "name": "open_bracket_modifier",
                                "value": 0
                            }
                        }
                    ],
                    "to_if_alone": [
                        {
                            "key_code": "open_bracket"
                        }
                    ],
                    "type": "basic"
                },
                {
                    "conditions": [
                        {
                            "type": "variable_if",
                            "name": "open_bracket_modifier",
                            "value": 1
                        }
                    ],
                    "from": {
                        "key_code": "a"
                    },
                    "to": [
                        {
                            "key_code": "e",
                            "modifiers": [
                                "left_alt"
                            ]
                        },
                        {
                            "key_code": "a"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
